{
  "name": "bowenislandtattooshop",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:server": "tsx watch lib/server.ts",
    "build:server": "tsc -p tsconfig.server.json",
    "start:server": "node dist/server.js",
    "test": "jest",
    "test:backend": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config=jest.backend.config.mjs",
    "test:frontend": "jest --config=jest.frontend.config.mjs",
    "test:watch": "jest --watch",
    "test:backend:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config=jest.backend.config.mjs --watch",
    "test:frontend:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config=jest.frontend.config.mjs --watch",
    "test:coverage": "jest --coverage",
    "test:backend:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules COVERAGE=true jest --config=jest.backend.config.mjs",
    "test:frontend:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules COVERAGE=true jest --config=jest.frontend.config.mjs",
    "dev:all": "concurrently \"npm:dev\" \"npm:dev:server\"",
    "test:integration": "jest --testPathPattern=tattoorequestFlow.test.tsx"
  },
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/multipart": "^8.0.0",
