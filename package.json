    "next": "15.3.2",
    "next-auth": "^4.24.11",
    "next-cloudinary": "^6.16.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.3",
    "react-icons": "^5.5.0",
    "square": "^42.2.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
