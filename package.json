    "daisyui": "^5.0.35",
    "dotenv": "^16.5.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "msw": "^2.8.4",
    "pino-pretty": "^13.0.0",
    "prisma": "^6.8.2",
    "supertest": "^7.1.1",
